{
  "name": "rsbuild-plugin-react-inspector",
  "version": "0.0.2",
  "description": "ðŸ“¦ ðŸ“¦ rsbuild plugin react inspector",
  "scripts": {
    "build": "tsup && cp -r ./src/core ./dist && cp -r ./src/client ./dist",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "format": "prettier --write .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp -r && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src/client/load.js",
    "src/core/applyInspector.js"
  ],
  "keywords": [
    "rsbuild",
    "plugin",
    "print"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rsbuild/core": "^0.2.15",
    "@types/node": "^20.10.6",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-relaxed-ts": "^2.0.34",
    "esno": "^0.17.0",
    "prettier": "^3.1.1",
    "tsup": "^7.3.0",
    "typescript": "^5.3.3",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=14",
    "@rsbuild/core": ">=0.2.13"
  },
  "dependencies": {
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@nozomuikuta/h3-cors": "^0.2.2",
    "@rsbuild/shared": "^0.0.17",
    "h3": "^1.9.0",
    "launch-editor-middleware": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
